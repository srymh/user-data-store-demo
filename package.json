{
  "name": "user-data-store-demo",
  "version": "0.1.4",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "electron-store": "5.2.0",
    "localforage": "^1.9.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "user-data-store": "git+https://github.com/srymh/user-data-store#v0.1.5",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "cross-env BROWSER=NONE react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron --inspect=5858 --remote-debugging-port=9223 .\"",
    "electron:build": "yarn build && tsc -p electron && electron-builder",
    "test:main": "jest --watch --config ./electron/jest.config.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": ".",
  "main": "build/electron/main.js",
  "build": {
    "appId": "srymh.user-data-store-demo",
    "productName": "User Data Store Demo",
    "copyright": "Copyright Â© 2020 srymh",
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis",
      "icon": "build/assets/icons/app.ico"
    }
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "ts-jest": "^26.4.3",
    "wait-on": "^5.2.0"
  }
}
